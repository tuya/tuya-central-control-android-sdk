{
  "plugins": [
    "js-sequence-diagram-full@>=0.3.1",
    "prism",
    "-highlight",
    "anchors",
    "splitter",
    "github",
    "anchor-navigation-ex",
    "flexible-alerts",
    "expandable-chapters-small",
    "favicon",
    "pageview-count",
    "hide-element",
    "sf-score",
    "mermaid-gb3"
  ],
  "pluginsConfig": {
    "prism": {
      "css": [
        "prismjs/themes/prism-okaidia.css"
      ],
      "lang": {
        "java": "java"
      },
      "ignore": [
        "sequence"
      ]
    },
    "github": {
      "url": "https://github.com/TuyaInc/tuyasmart_android_device_central_sdk"
    },
    "anchor-navigation-ex": {
      "showLevel": true,
      "associatedWithSummary": false,
      "showGoTop": true
    },
    "favicon": {
      "shortcut": "zh-hans/resource/images/bitbug_favicon.ico"
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    },
    "flexible-alerts": {
      "style": "flat",
      "note": {
        "label": {
          "zh-hans": "提示",
          "en": "Note"
        }
      },
      "tip": {
        "label": {
          "zh-hans": "Tips",
          "en": "Tip"
        }
      },
      "warning": {
        "label": {
          "zh-hans": "警告",
          "en": "Warning"
        }
      },
      "danger": {
        "label": {
          "zh-hans": "注意",
          "en": "Attention"
        }
      }
    }
  }
}